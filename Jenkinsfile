pipeline {
  agent any
  stages {
     stage("checkout") {
           steps {
		   echo 'checkout the code the....'
           }
          } 
     stage("build") {
           steps {
		   echo 'building the application....'
           }
          }
      stage("Test") {
           steps {
		   echo 'Testing the application....'
           }
          }		 
      stage("deploy") {
           steps {
		   echo 'deploying the application....'
           }
          }		  
	  stage("cleanup") {
           steps {
		   echo 'cleanup the application....'
           }
          }
      }
}
